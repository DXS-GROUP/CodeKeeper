QDialog {
    background-color: #1f1f28;
    border-width: 2px;
    border-top-left-radius: 12px;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    border-bottom-left-radius: 12px;
}
QWidget {
    background-color: #1F1F28;
    border-width: 2px;
    border-color: #DCD7BA;
    border-top-left-radius: 12px;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    border-bottom-left-radius: 12px;
}
QMainWindow {
    background-color: #1F1F28;
    color: #c8d3f5;
}

QStatusBar {
    background-color: #1F1F28;
    border-top-left-radius: 12px;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    border-bottom-left-radius: 12px;
}

QStatusBar::QSizeGrip {
    background-color: transparent;
}

QSizeGrip {
    background-color: transparent;
    border-radius: 10px;
}

QMarkdownTextEdit {
    line-height: 10px;
    background-color: #1f1f28;
    border-radius: 10px;
}

QMarkdownTextEdit::lineNumber {
    background-color: #5260bb;
    border-radius: 10px;
}

QMarkdownTextEdit::lineNumber:selected {
    color: #DCD7BA;
    background-color: #1f1f28;
    border-radius: 10px;
}

QMarkdownTextEdit::lineNumber::selected:active {
    color: #05ff00;
    background-color: #1f1f28;
    border-radius: 10px;
}

QFileDialog {
    background-color: #1a1b26;
}

QFileDialog QLabel {
    color: #a9b1d6;
}

QFileDialog QListView {
    background-color: #1a1b26;
    color: #a9b1d6;
}

QFileDialog QTreeView {
    background-color: #1a1b26;
    color: #a9b1d6;
}

QFileDialog QListView::item {
    padding: 6px;
}

QFileDialog QListView::item:hover {
    background: #34354a;
}

QFileDialog QListView::item:selected {
    background: #2a2b3b;
}

QFileDialog QPushButton {
    background-color: #565f89;
    color: #DCD7BA;
    border: 1px solid #565f89;
    border-radius: 5px;
    height: 30px;
    min-width: 80px;
}

QFileDialog QPushButton:hover {
    background-color: #7785a2;
    border: 1px solid #7785a2;
}

QCheckBox {
    color: #c8d3f5;
}

QFontComboBox {
    color: #c8d3f5;
    background-color: #1F1F28;
    alternate-background-color: #2D4F67;
}

QComboBox {
    color: #c8d3f5;
    background-color: #1F1F28;
    alternate-background-color: #2D4F67;
}

QSpinBox {
    color: #c8d3f5;
    background-color: #1F1F28;
    alternate-background-color: #2D4F67;
}

QTabWidget {
    background-color: #1f1f28;
    font-size: 9px;
}

QTabWidget::pane {
    border: 0px solid lightgray;
    border-radius: 10px;
    top:-1px; 
    background: #1F1F28;; 
} 

QMenu {
    background-color: 2D4F67;
    color: #fff;
    border: 6px solid 2D4F67;
    border-radius: 0px;
}

QMenu::separator {
    background-color: #2D4F67;
    border: 0pxl solid #2D4F67;
    border-radius: 1px;
}

QMenu::item {
    border-radius: 0px;
    background-color: 2D4F67;
    color: #fff;
    padding: 5px 10px;
}

QMenu::item:selected {
    border-radius: 10px;
    color: #2D4F67;
    text-decoration: underline;
}

QToolButton {
    background-color: #1F1F28;
    color: #fff;
    border-radius: 10px;
    padding: 5px 10px;
}

QToolButton:pressed {
    background-color: #505a6a;
}

QTabWidget::tab-bar { alignment: center; }

QTabBar::tab {
    height: 15px;
    width: 100px;
    font-size: 10px;
    background: #1f1f28; 
    color: #d9dee8;
    border: 0px solid lightgray; 
    border-radius: 5px;
    padding: 5px;
    margin: 5px;
    margin-bottom: 5px; 
} 

QTabBar::tab:selected {
    height: 15px;
    width: 100px;
    font-size: 10px;
    color: #d9dee8;
    background: #2D4F67; 
    border-radius: 5px;
    padding: 5px;
    margin-bottom: 5px; 
}

QToolButton {
        background-color: #1f1f28;
        selection-background-color: #2D4F67;
        color: #c8d3f5;
        border-width: 2px;
        border-style: solid;
        border-radius: 10px;
        alignment: center;
        border-color: #1f1f28;
}



QPushButton {
        background-color: #2D4F67;
        selection-background-color: #2D4F67;
        color: #c8d3f5;
        border-width: 2px;
        border-style: solid;
        border-radius: 10px;
        alignment: center;
        border-color: #1f1f28;
}

QPushButton::ToolTip {
    color: #DCD7BA;
}

QLabel {
    color: #c8d3f5;
}

QLineEdit {
        background-color: #2D4F67;
        selection-background-color: #999999;
        selection-color: #c8d3f5;
        color: #c8d3f5;
        border-width: 0px;
        border-style: solid;
        border-radius: 10px;
        border-color: #1f1f28;
        alternate-background-color: #181818;
}

QPlainTextEdit {
        background-color: #1F1F28;
        selection-background-color: #999999;
        selection-color: #c8d3f5;
        color: #c8d3f5;
        border-width: 0px;
        border-style: solid;
        border-radius: 10px;
        border-color: #1f1f28;
        alternate-background-color: #181818;
}

QTextBrowser {
        line-height: 10px;
        background-color: #1F1F28;
        selection-background-color: #999999;
        selection-color: #c8d3f5;
        color: #c8d3f5;
        border-width: 0px;
        border-style: solid;
        border-radius: 10px;
        border-color: #1f1f28;
        alternate-background-color: #181818;
}


QListWidget {
        background-color: #1f1f28;
        color: #c8d3f5;
        selection-color: #c8d3f5;
        border-width: 0px;
        border-style: solid;
        border-radius: 5px;
        border-color: #1f1f28;
        alternate-background-color: #181818;
}

QListWidget::item:selected {
    color: #DCD7BA;
    background-color: #2D4F67;
    border-radius: 5px;
}

QGroupBox {
    color: #c8d3f5;
    background-color: #1f1f28;
    border-radius: 10px;
}

QTreeView {
        background-color: #1f1f28;
        selection-background-color: #2D4F67;
        selection-color: #c8d3f5;
        color: #c8d3f5;
        border-width: 0px;
        border-style: solid;
        border-radius: 10px;
        border-color: #2D4F67;
        alternate-background-color: #2D4F67;
} 

QTreeView::item:alternate {
    height: 14px;
    background-color: #1f1f28;
    border-radius: 0px;
}

QTreeView::item:selected {
    height: 14px;
    background-color: #2d4f67;
    border: 0px solid #2D4F67;
    border-radius: 0px;
}

QTreeView::item:selected:!active {
    height: 14px;
    background-color: #1F1F28;
    color: #ffffff;
    border-radius: 0px;
}

QTreeView::item:selected:active {
    height: 14px;
    background-color: #2d4f67;
    color: #ffffff;
    border-radius: 0px;
}

QTreeView::item:hover {
    height: 14px;
    background-color: #1F1F28;
    border-radius: 0px;
}

QHeaderView {
    border: 1px solid #1f1f28;
    background-color: #1f1f28;
    color: #DCD7BA;
}

QHeaderView::section {
    border: 1px solid #1f1f28;
    background-color: #1f1f28;
    color: #DCD7BA;
}

QHeaderView::section:checked {
    border: 1px solid #1f1f28;
    background-color: #1f1f28;
    color: #DCD7BA;
}

QHeaderView::section:checked:horizontal {
    border: 1px solid #1f1f28;
    background-color: #1f1f28;
    color: #DCD7BA;
}

QHeaderView::section:checked:vertical {
    border: 1px solid #1f1f28;
    background-color: #1f1f28;
    color: #DCD7BA;
}

QProgressBar {
    color: #1F1F28;
    background-color: #ff757f;
    border-radius: 5px;
}

QProgressBar::chunk {
    color: #1F1F28;
    background-color: #c3e88d;
    border-radius: 5px;
}



QScrollBar:vertical {
    background: #1F1F28;
    border-radius: 3px;
    width: 6px;
    margin: 3px 0px 3px 0px;
}

QScrollBar:horizontal {
    background: #1F1F28;
    border-radius: 3px;
    height: 6px;
    margin: 0px 3px 3px 0px;
}

QScrollBar::handle:vertical {
    border-radius: 3px;
    background: #666c72;
    height: 6px;
}

QScrollBar::handle:horizontal {
    border-radius: 3px;
    background: #666c72;
    min-width: 20px;
}

QScrollBar::handle:vertical:hover, QScrollBar::handle:horizontal:hover {
    border-radius: 2px;
    background: #666c72;
}

QScrollBar::add-line, QScrollBar::sub-line {
    border-radius: 2px;
    background: #1F1F28;
}

QScrollBar::add-line:vertical {
    border-radius: 2px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
    height: 0px;
}

QScrollBar::sub-line:vertical {
    border-radius: 2px;
    subcontrol-position: top;
    subcontrol-origin: margin;
    height: 0px;
}

QScrollBar::add-line:horizontal {
    border-radius: 2px;
    subcontrol-position: right;
    subcontrol-origin: margin;
    width: 0px;
}

QScrollBar::sub-line:horizontal {
    border-radius: 2px;
    subcontrol-position: left;
    subcontrol-origin: margin;
    width: 0px;
}

QScrollBar::add-page, QScrollBar::sub-page {
    background: #1F1F28;
}
