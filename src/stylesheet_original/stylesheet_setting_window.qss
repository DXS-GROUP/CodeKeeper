}

QTabWidget::pane {
    border: 0px solid lightgray;
    border-radius: 10px;
    top:-1px; 
    background: #3f4463;; 
} 

QMenu {
    background-color: #545c7e;
    color: #fff;
    border: 6px solid #3f4463;
    border-radius: 10px;
}

QMenu::separator {
    background-color: #84a0bf;
    border: 1pxl solid #84a0bf;
    border-radius: 1px;
}

QMenu::item {
    border-radius: 10px;
    background-color: #545c7e;
    color: #fff;
    padding: 5px 10px;
}

QMenu::item:selected {
    border-radius: 10px;
    color: #84a0bf;
    text-decoration: underline;
}

QToolButton {
    background-color: #545c7e;
    color: #fff;
    border-radius: 10px;
    padding: 5px 10px;
}

QToolButton:pressed {
    background-color: #505a6a;
}

QTabWidget::tab-bar { alignment: center; }

QTabBar::tab {
    width: 100px;
    font-size: 10px;
    background: #3f4463; 
    color: #d9dee8;
    border: 0px solid lightgray; 
    border-radius: 5px;
    padding: 5px;
    margin: 5px;
    margin-bottom: 5px; 
} 

QTabBar::tab:selected { 
    width: 100px;
    font-size: 10px;
    color: #d9dee8;
    background: #545c7e; 
    border-radius: 5px;
    padding: 5px;
    margin-bottom: 5px; 
}

QToolButton {
        background-color: #3f4463;
